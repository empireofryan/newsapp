<div class="row style2" id="ny_times">
  <div class="col-md-12">
    <p class="site">
      <img src='https://upload.wikimedia.org/wikipedia/commons/7/77/The_New_York_Times_logo.png'
      style='width:535px; margin-top:0px'><br>
      </p>

    <div  style="font-size:.9em; text-align:center">
      <% @nytimes.past_day.each_with_index do |a, i| %>
      <a href='<%=a.url%>'>
        <div class="animatedParent">
          <h2 class='animated bounceInDown' data-id='<%=i+1%>'> <%= a.title %> </h2>
        </div>
      </a>
      <% end %>
    </div>
  </div>
</div>
