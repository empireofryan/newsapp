<div class="row style2" id="espn">
  <div class="col-md-12">
    <p class="site">
      <img src='http://logos-download.com/wp-content/uploads/2016/05/ESPN_logo_wordmark.png'
      style='width:235px; margin-top:0px'><br>
    </p>
      <div style="font-size:.9em; text-align:center">
        <% @espns.past_day.each_with_index do |a, i| %>
        <a href='<%=a.url%>'>
          <div class="animatedParent">
            <h2 class='animated bounceInDown' data-id='<%=i+1%>'> <%= a.title %> </h2>
          </div>
        </a>
        <% end %>
      </div>
    </div>
  </div>
