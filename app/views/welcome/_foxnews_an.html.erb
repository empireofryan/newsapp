
<div class="row style2" id="fox_news">
  <div class="col-md-12">
    <p class="site">
      <img src='http://commonwealthmagazine.org/wp-content/uploads/sites/2/2015/12/fox-news-logo.png'
      style='width:235px; margin-top:100px'><br>
    </p>
<div style="font-size:.9em; text-align:center; margin-left:15%; margin-right:15%">
  <% @foxnews.past_day.each_with_index do |a, i| %>
  <a href='<%=a.url%>'>
    <div class="animatedParent">
      <h2 class='animated bounceInDown' data-id='<%=i+1%>'> <%= a.title %> </h2>
    </div>
  </a>
  <% end %>
</div>
</div>
</div>
