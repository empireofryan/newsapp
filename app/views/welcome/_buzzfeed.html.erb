<div class="row style2">
  <div class="col-md-12">
    <p class="site">
      <img src='https://upload.wikimedia.org/wikipedia/commons/e/e4/BuzzFeed.svg'
      style='width:235px; margin-top:100px'><br>
      Latest News</p>
    <p style="font-size:.9em; text-align:center">
      <% @buzzfeeds.past_day.each do |a| %>
      <a href='<%=a.url%>'>  <%= a.title %> </a>
      <% end %>
    </p>
  </div>
</div>
